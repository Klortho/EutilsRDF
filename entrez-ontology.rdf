<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY ncbi "http://rdf.ncbi.nlm.nih.gov/">
  <!ENTITY entrez "&ncbi;entrez/">
  <!ENTITY pmc "&ncbi;pmc/">
]>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:ncbi="&ncbi;"
         xmlns:entrez="&entrez;"
         xmlns:pmc="&pmc;">
  
  <owl:Ontology rdf:about='http://chrismaloney.org/test/ontology'
                dc:title="Sample ontology for Semantic Web class"/>

  <!-- Entrez databases -->
  <rdfs:Class rdf:about="&entrez;db"
              rdfs:label="database"
              rdfs:comment="NCBI Entrez database"/>

  <!-- A class for Entrez links -->
  <rdfs:Class rdf:about="&entrez;link"
              rdfs:label="link"
  
  <rdfs:Class rdf:about="&pmc;article"
              rdfs:label="article"
              rdfs:comment="PMC article"/>
  
</rdf:RDF>